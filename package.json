{
    "name": "inwt-python-ide",
    "displayName": "INWT Python IDE",
    "description": "Extension bundle to develop Python applications for Data Science.",
    "publisher": "inwt",
    "repository": {
        "url": "https://github.com/inwtlab/inwt-python-ide"
    },
    "version": "0.4.0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Extension Packs",
        "Keymaps"
    ],
    "activationEvents": [
        "onLanguage:python"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "inwt-python-ide.jupyterExecAndStep",
                "title": "INWT: Send selection to interactive Python and step."
            },
            {
                "command": "inwt-python-ide.debugExecAndStep",
                "title": "INWT: Send selection to debug console and step."
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+tab",
                "command": "workbench.action.quickOpen"
            },
            {
                "key": "ctrl+tab",
                "command": "workbench.action.quickOpenNavigateNextInFilePicker",
                "when": "inFilesPicker && inQuickOpen"
            },
            {
                "key": "ctrl+shift+tab",
                "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
                "when": "inFilesPicker && inQuickOpen"
            },
            {
                "key": "ctrl+p",
                "command": "workbench.action.openRecent"
            },
            {
                "key": "ctrl+p",
                "command": "workbench.action.quickOpenNavigateNextInRecentFilesPicker",
                "when": "inQuickOpen && inRecentFilesPicker"
            },
            {
                "key": "ctrl+f12",
                "command": "workbench.action.navigateBack"
            },
            {
                "key": "ctrl+down",
                "command": "spaceBlockJumper.moveDown",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+up",
                "command": "spaceBlockJumper.moveUp",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+down",
                "command": "spaceBlockJumper.selectDown",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+up",
                "command": "spaceBlockJumper.selectUp",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+enter",
                "command": "inwt-python-ide.debugExecAndStep",
                "when": "editorTextFocus && inDebugMode && !replaceInputFocussed && editorLangId == 'python'"
            },
            {
                "key": "ctrl+enter",
                "command": "inwt-python-ide.jupyterExecAndStep",
                "when": "editorTextFocus && !inDebugMode && !replaceInputFocussed && editorLangId == 'python'"
            }
        ]
    },
    "extensionDependencies": [
        "ms-python.python",
        "ms-toolsai.jupyter"
    ],
    "extensionPack": [
        "ms-python.vscode-pylance",
        "guyskk.language-cython",
        "VisualStudioExptTeam.vscodeintellicode",
        "TabNine.tabnine-vscode",
        "christian-kohler.path-intellisense",
        "CoenraadS.bracket-pair-colorizer-2",
        "GrapeCity.gc-excelviewer",
        "mikestead.dotenv",
        "ms-azuretools.vscode-docker",
        "ms-vscode-remote.remote-containers",
        "ms-vscode-remote.remote-ssh",
        "ms-vscode-remote.remote-ssh-edit",
        "ms-vsliveshare.vsliveshare",
        "GitHub.vscode-pull-request-github",
        "eamodio.gitlens",
        "stkb.rewrap",
        "streetsidesoftware.code-spell-checker",
        "streetsidesoftware.code-spell-checker-german",
        "usernamehw.errorlens",
        "Gruntfuggly.todo-tree",
        "jmfirth.vsc-space-block-jumper"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.52.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.15.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    }
}
